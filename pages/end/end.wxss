/* pages/end/end.wxss */

page {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    background: url('https://s2.ax1x.com/2019/04/11/A7lTxO.png') no-repeat;
    background-size: 100% 100%;
}

.page {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: flex-start;
}

.v1 {
    margin-top: 10rpx;
}

.canvas_img {
    width: 105%;
    height: 100%;
    border: 10rpx solid #f75451;
    border-radius: 50rpx;
    background: #fff;
}

.btn {
    animation: btn 3s linear infinite;
}

@keyframes btn {
    0% {
        transform: scale(1);
    }

    25% {
        transform: scale(1);
    }

    35% {
        transform: scale(1.2);
    }

    50% {
        transform: scale(1);
    }

    65% {
        transform: scale(1.2);
    }

    80% {
        transform: scale(1);
    }

    100% {
        transform: scale(1);
    }
}

.img1 {
    width: 300rpx;
    height: 122rpx;
}

.img2 {
    width: 210rpx;
    height: 86rpx;
}
